{
  "hosting": [
    {
      "target": "hofsdev",
      "public": "public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "hofsdevcv",
      "public": "public/cv",
      "redirects": [
        {
          "source": "/",
          "destination": "/cv.pdf",
          "type": 301
        }
      ]
    }
  ]
}
